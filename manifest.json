{
	"name" : "Hacker News Inline Reply",
	"version" : "1.0.2",
	"manifest_version" : 2,
	"description" : "A plugin to make replying on Hacker news inline and easy",
	"icons" : {
		"16" : "logo16.png",
		"48" : "logo48.png",
		"128" : "logo128.png"
	},
	"content_scripts" : [
		{
			"matches" : ["http://news.ycombinator.com/item?*"],
			"css" : [],
			"js" : ["jquery.js", "hnreply.js"]
		}
	]
}